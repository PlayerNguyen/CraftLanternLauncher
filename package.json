{
  "name": "crafting-table-launcher",
  "description": "description",
  "authors": "PlayerNguyen",
  "version": "1.0.0-alpha",
  "license": "MIT",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node ./scripts/check-build.ts && electron ./dist/src/index.js",
    "dev": "npm run clean && cross-env NODE_ENV=development concurrently \"npm:watch-*\" --exit",
    "watch-render": "ts-node ./scripts/watch-render",
    "watch-electron": "wait-on file:./dist/render && concurrently \"tsc -w\" \"wait-on file:./dist/src && electronmon ./dist/src/index.js\" ",
    "build": "ts-node ./scripts/build-application.ts && tsc",
    "package": "ts-node ./scripts/pack-application.ts",
    "clean": "ts-node ./scripts/clean-application.ts"
  },
  "dependencies": {
    "needle": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.8.2",
    "@parcel/core": "^2.8.2",
    "@types/needle": "^3.2.0",
    "@types/react": "^18.0.26",
    "@types/uuid": "^9.0.0",
    "chalk": "4.1.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-packager": "^17.1.1",
    "electronmon": "^2.0.2",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  }
}
